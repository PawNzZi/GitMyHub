/* pages/login/login.wxss */
.contains-view{
  background: #f8f8f9;
  height: 100vh;
}
.login-view{
  position: absolute;
  top: 320rpx;
  background: white;
  height: 350rpx;
  z-index: 1;
  width: 600rpx;
  border-radius: 9rpx;
  left: 10%;
  display: flex;
  flex-direction: column;
  align-items: center;

}
.token-view{
  display: flex;
  flex-direction: row;
  margin-top: 50rpx;
}
.input{
  margin-left: 60rpx;
  width: 200rpx;
  color:#495060;
}
.commit-token{
  background: #0387FE;
  height: 70rpx;
  width: 200rpx;
  text-align: center;
  line-height: 70rpx;
  color: white;
  border-radius: 70rpx;
  margin-top: 35rpx;
}
.logo{
  width: 200rpx;
  height: 200rpx;
  margin-top: 30rpx;
}
.top-view{
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding-top: 20rpx;
}