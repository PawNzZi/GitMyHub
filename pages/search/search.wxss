/* pages/search/search.wxss */
.history-view{
  padding: 30rpx;
  display: flex;
  flex-direction: row;
  flex-flow: wrap;
}
.search-history{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-left: 30rpx;
  padding-right: 30rpx;
  margin-top: 35rpx;
}
.search-right{
  display: flex;
  flex-direction: row;
}
.history-key{
  color: #80848f;
  text-align: center;
  line-height: 35rpx;
  background: #f8f8f9;
  border-radius: 60rpx;
  font-size: 22rpx;
  margin-left: 15rpx;
  margin-top: 15rpx;
  padding: 10rpx 10rpx;
}
.icon-view{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 150rpx;
}
.tipImg{
  width: 500rpx;
  height: 307rpx;
}